{
  "comments": true,
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ],
  "presets": [
    ["env", { "modules": false }]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-object-rest-spread",
        "syntax-dynamic-import",
        "transform-remove-strict-mode"
      ],
      "presets": [
        ["env", {
          "targets": { "node": "current" },
        }]
      ]
    }
  }
}
